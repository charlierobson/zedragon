;	vn.	122

	list	i,d


; Filespec:	DISPDATA


	org	$1b50+offset
	loc	$1b50+develop


; Variables

; How long in bytes each screen line is
;   (30 screens)
scrlen	=	20*30


	incl	D2:LINES04

	incl	D2:LINES59


; Display list for main screen

	assert	*=$34b0+develop

dlist	db	dlbl8,dlbl8,dlbl8

	db	dlbm0+dllms
	dw	schead

	db	dlbl1
	db	dlbm1+dlint+dllms
	dw	score

	db	dlbl1
	db	dlbl2+dlint
	db	dlbm2+dlhscr+dlint+dllms
ptr0	dw	line0
	db	dlbm2+dlhscr+dllms
	dw	line1
	db	dlbm2+dlhscr+dllms
	dw	line2
	db	dlbm2+dlhscr+dllms
	dw	line3
	db	dlbm2+dlhscr+dllms
	dw	line4
	db	dlbm2+dlhscr+dllms
	dw	line5
	db	dlbm2+dlhscr+dllms
	dw	line6
	db	dlbm2+dlhscr+dllms
	dw	line7
	db	dlbm2+dlhscr+dllms
	dw	line8
	db	dlbm2+dlhscr+dlint+dllms
	dw	line9
	db	dlbl2+dlint
	db	dlbl1+dlint

	db	dlbm1+dllms
	dw	air

	db	dljvb
	dw	dlist


; My own "SETVBV" routine
; Seems that old O/S routine screws up
;   screen sometimes, so this assures
;   it'll be right for all machines

	assert	*=$34e2+develop

setvbv

; Timer # * 2 for word offset
	asl	a
; Save it (INTEMP in O/S listing)
	sta	cdtmf4+1
; Save high byte of vector/count
	txa

; Wait till a little after WSYNC occurs
;   which means we can't have a VBLANK
;   any time soon
	ldx	#5
	sta	wsync
setvb2	dex
	bne	setvb2

; Retrieve index and stuff vector/count
	ldx	cdtmf4+1
	sta	cdtmv1-1,x
	tya
	sta	cdtmv1-2,x

	rts


; Character sets data

	org	$3600+offset
	loc	$3600+develop

; Playfield character set #2


chrst2
